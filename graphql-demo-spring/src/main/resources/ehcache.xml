<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://www.ehcache.org/ehcache.xsd"
         updateCheck="true"
         monitoring="autodetect"
         dynamicConfig="true">

    <cache name="authors_all"
           maxElementsInMemory="1" eternal="false" overflowToDisk="false"
           timeToLiveSeconds="300" timeToIdleSeconds="0" memoryStoreEvictionPolicy="LFU" transactionalMode="off">
    </cache>

    <cache name="authors"
           maxElementsInMemory="100" eternal="false" overflowToDisk="false"
           timeToLiveSeconds="300" timeToIdleSeconds="0" memoryStoreEvictionPolicy="LFU" transactionalMode="off">
    </cache>

    <cache name="books_all"
           maxElementsInMemory="1" eternal="false" overflowToDisk="false"
           timeToLiveSeconds="300" timeToIdleSeconds="0" memoryStoreEvictionPolicy="LFU" transactionalMode="off">
    </cache>

    <cache name="books"
           maxElementsInMemory="100" eternal="false" overflowToDisk="false"
           timeToLiveSeconds="300" timeToIdleSeconds="0" memoryStoreEvictionPolicy="LFU" transactionalMode="off">
    </cache>

    <cache name="comments_all"
           maxElementsInMemory="1" eternal="false" overflowToDisk="false"
           timeToLiveSeconds="300" timeToIdleSeconds="0" memoryStoreEvictionPolicy="LFU" transactionalMode="off">
    </cache>

    <cache name="comments"
           maxElementsInMemory="100" eternal="false" overflowToDisk="false"
           timeToLiveSeconds="300" timeToIdleSeconds="0" memoryStoreEvictionPolicy="LFU" transactionalMode="off">
    </cache>

</ehcache>